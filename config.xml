<?xml version='1.0' encoding='utf-8'?>
<widget id="seo.ranking" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Seo Ranking</name>
    <description>
        SEO Ranking is the successor of the popular SEO Search Ranking. It allows you to easily monitor your websites search enigne ranking for any keywords.
    </description>
    <author email="info@itda.ge" href="https://itda.ge">
        ITDA.GE
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />

  <icon src="res/icon/android/ldpi-background.png" density="ldpi" />
  <icon src="res/icon/android/mdpi-background.png" density="mdpi" />
  <icon src="res/icon/android/hdpi-background.png" density="hdpi" />
  <icon src="res/icon/android/xhdpi-background.png" density="xhdpi" />
  <icon src="res/icon/android/xxhdpi-background.png" density="xxhdpi" />
  <icon src="res/icon/android/xxxhdpi-background.png" density="xxxhdpi"  />
    </platform>

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
</widget>
